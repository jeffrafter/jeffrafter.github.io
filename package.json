{
  "name": "jeffrafter",
  "description": "Jeff Rafter's blog",
  "version": "1.0.0",
  "private": true,
  "author": "Jeff Rafter <jeffrafter@gmail.com>",
  "keywords": [
    "gatsby",
    "blog",
    "typescript",
    "styled-components"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -b master",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@tufte-markdown/remark-figure-transformer": "^1.0.0",
    "@tufte-markdown/remark-sidenotes": "^1.0.0",
    "@tufte-markdown/remark-wrap-in-section": "^2.0.1",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/styled-components": "^4.4.0",
    "babel-plugin-styled-components": "^1.10.6",
    "classnames": "^2.2.6",
    "gatsby": "^2.17.11",
    "gatsby-image": "^2.2.31",
    "gatsby-plugin-canonical-urls": "^2.1.14",
    "gatsby-plugin-feed": "^2.3.20",
    "gatsby-plugin-google-analytics": "^2.1.25",
    "gatsby-plugin-manifest": "^2.2.27",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-sharp": "^2.2.37",
    "gatsby-plugin-styled-components": "^3.1.12",
    "gatsby-plugin-typescript": "^2.1.16",
    "gatsby-remark-autolink-headers": "^2.1.17",
    "gatsby-remark-copy-linked-files": "^2.1.29",
    "gatsby-remark-images": "^3.1.30",
    "gatsby-remark-prismjs": "^3.3.22",
    "gatsby-remark-responsive-iframe": "^2.2.26",
    "gatsby-remark-smartypants": "^2.1.15",
    "gatsby-remark-tufte": "^1.0.4",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-transformer-json": "^2.2.17",
    "gatsby-transformer-remark": "^2.6.33",
    "gatsby-transformer-sharp": "^2.3.3",
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "rimraf": "^3.0.0",
    "styled-components": "^4.4.1",
    "tslint": "^5.20.1",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  }
}
